CREATE TABLE `Subscription` (
  `_rallyAPIMajor` varchar(300) DEFAULT NULL,
  `_rallyAPIMinor` varchar(300) DEFAULT NULL,
  `_ref` varchar(300) DEFAULT NULL,
  `_refObjectUUID` varchar(300) DEFAULT NULL,
  `_objectVersion` varchar(300) DEFAULT NULL,
  `_refObjectName` varchar(300) DEFAULT NULL,
  `CreationDate` varchar(300) DEFAULT NULL,
  `_CreatedAt` varchar(300) DEFAULT NULL,
  `ObjectID` varchar(20) NOT NULL,
  `ObjectUUID` varchar(300) DEFAULT NULL,
  `VersionId` varchar(300) DEFAULT NULL,
  `ApiKeysEnabled` int(20) DEFAULT NULL,
  `EmailEnabled` int(20) DEFAULT NULL,
  `ExpirationDate` varchar(300) DEFAULT NULL,
  `JSONPEnabled` int(20) DEFAULT NULL,
  `MaximumCustomUserFields` int(20) DEFAULT NULL,
  `MaximumProjects` int(20) DEFAULT NULL,
  `Modules` varchar(440) DEFAULT NULL,
  `Name` varchar(300) DEFAULT NULL,
  `PasswordExpirationDays` int(20) DEFAULT NULL,
  `PreviousPasswordCount` int(20) DEFAULT NULL,
  `ProjectHierarchyEnabled` int(20) DEFAULT NULL,
  `SessionTimeoutSeconds` varchar(300) DEFAULT NULL,
  `StoryHierarchyEnabled` int(20) DEFAULT NULL,
  `StoryHierarchyType` varchar(300) DEFAULT NULL,
  `SubscriptionID` int(20) DEFAULT NULL,
  `SubscriptionType` varchar(300) DEFAULT NULL,
  `WebhooksEnabled` int(20) DEFAULT NULL,
  `ZuulID` varchar(300) DEFAULT NULL,
  `time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`ObjectID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
----------
