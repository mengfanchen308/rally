  `ObjectID` varchar(20) NOT NULL,
  `ObjectUUID` varchar(300) DEFAULT NULL,
  `VersionId` varchar(300) DEFAULT NULL,
  `Subscription/Subscription` varchar(300) DEFAULT NULL,
  `Workspace/Workspace` varchar(300) DEFAULT NULL,
  `Description` text,
  `DisplayColor` varchar(300) DEFAULT NULL,
  `Expedite` int(20) DEFAULT NULL,
  `FormattedID` varchar(300) DEFAULT NULL,
  `LastUpdateDate` varchar(300) DEFAULT NULL,
  `LatestDiscussionAgeInMinutes` varchar(300) DEFAULT NULL,
  `Name` varchar(300) DEFAULT NULL,
  `Notes` text,
  `Owner/User` varchar(300) DEFAULT NULL,
  `Ready` int(20) DEFAULT NULL,
  `RevisionHistory/RevisionHistory` varchar(300) DEFAULT NULL,
  `Actuals` decimal(8,2) DEFAULT NULL,
  `Blocked` int(20) DEFAULT NULL,
  `BlockedReason` varchar(300) DEFAULT NULL,
  `Estimate` decimal(8,2) DEFAULT NULL,
  `Iteration/Iteration` varchar(300) DEFAULT NULL,
  `Project/Project` varchar(300) DEFAULT NULL,
  `Rank` decimal(8,2) DEFAULT NULL,
  `Recycled` int(20) DEFAULT NULL,
  `Release/Release` varchar(300) DEFAULT NULL,
  `State` varchar(300) DEFAULT NULL,
