CREATE TABLE `error_object` (
  `id` varchar(75) NOT NULL,
  `name` varchar(45) NOT NULL,
  `reason` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
